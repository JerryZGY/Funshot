<UserControl x:Class="Funshot.Pages.Page_Start"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Funshot.Pages" xmlns:c="http://schemas.microsoft.com/kinect/2014"
             mc:Ignorable="d" d:DesignWidth="1366" d:DesignHeight="768" Loaded="Page_Start_Loaded">
    <UserControl.Resources>
        <ResourceDictionary Source="../Resources/Res_Page_Start.xaml"/>
    </UserControl.Resources>
    <c:KinectRegion x:Name="Reg">
        <Grid x:Name="Grid_Main" Style="{DynamicResource Grid_Main}">
            <Image x:Name="Img_Title" Style="{DynamicResource Img_Title}"/>
            <Button x:Name="Btn_Start" Style="{DynamicResource Btn_Start}" Click="Btn_Start_Click"/>
        </Grid>
    </c:KinectRegion>
</UserControl>